{
  "manifest_version": 2,
  "name": "Gitpod Window",
  "version": "1.0.0",
  "description": "Gitpod Window disables browser shortcuts for you to prevent unwant behavior",
  "author": "Jerome Wu <jeromewus@gmail.com>",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "commands": {},
  "browser_action": {},
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*/*",
        "*://gitlab.com/*/*"
      ] 
    }
  ]
}
